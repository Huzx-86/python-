for i in range(1,10):
    for j in range(1,10):
        if i <= j:
            if j == 9:
                print(i,"*",j,"=",i*j,end="\t")
                print("\n",end="")
            else:
                print(i,"*",j,"=",i*j,end="\t")

print("----------------------------------------------------------------------------------------------------------------------------------------------------------------")

for i in range(1,10):
    for j in range(1,i+1):
        print(j,"*",i,"=",i*j,end="\t")
    print("")

print("----------------------------------------------------------------------------------------------------------------------------------------------------------------")

for i in range(1,10):
    for j in range(1,i+1):
        if j == i:
            print(i,"*",j,"=",i*j,end="\t")
            print("\n",end="")
        else:
            print(i,"*",j,"=",i*j,end="\t")
